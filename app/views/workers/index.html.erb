<main class='main'>
	<div class='container clearfix'>
		<h3 class='section-title'>VR Film School Team</h3>
		<div class='column max-width-sml clearfix'>
			<% @workers.each do |worker| %>
				<div class="team-img-cont <%= worker.custom_class %>">
					<img class='team-img' src="<%= asset_path("#{worker.img}") %>" />
					<p class='team-info name'><%= worker.name %></p>
					<p class='team-info'><%= worker.bio %></p>
				</div>
			<% end %>
		</div>
	</div>
	<%= render 'partials/map' %>
</main>