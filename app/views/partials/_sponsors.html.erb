<div class='container clearfix'>
	<h3 class='section-title'>SPONSORS</h5>
	<div class='column full'>
		<div class='sponsor-cont clearfix'>
			<% @other.each do |sponsor| %>
				<div class='sponsor-img-cont duncan'>
					<img class='sponsor-img duncan' src="<%= asset_path("#{sponsor.img}") %>" />
				</div>
			<% end %>
		</div>
		<div class='sponsor-cont clearfix'>
			<h6 class='section-subtitle sponsor'>DIAMOND</h6>
			<% @diamond.each do |sponsor| %>
				<div class='sponsor-img-cont diamond'>
					<img class='sponsor-img' src="<%= asset_path("#{sponsor.img}") %>" />
				</div>
			<% end %>
		</div>
		<div class='sponsor-cont clearfix'>
			<h6 class='section-subtitle sponsor'>RUBY</h6>
			<% @ruby.each do |sponsor| %>
				<div class='sponsor-img-cont diamond'>
					<img class='sponsor-img' src="<%= asset_path("#{sponsor.img}") %>" />
				</div>
			<% end %>
		</div>
		<div class='sponsor-cont clearfix'>
			<h6 class='section-subtitle sponsor'>MEDIA</h6>
			<% @media.each do |sponsor| %>
				<div class='sponsor-img-cont'>
					<img class='sponsor-img' src="<%= asset_path("#{sponsor.img}") %>" />
				</div>
			<% end %>
		</div>
	</div>
</div>